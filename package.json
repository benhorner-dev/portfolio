{
	"name": "benhorner.dev",
	"version": "0.0.0-semantically-released",
	"type": "module",
	"devDependencies": {
		"@biomejs/biome": "2.2.4",
		"@commitlint/cli": "19.8.1",
		"@commitlint/config-conventional": "19.8.1",
		"@secretlint/secretlint-rule-preset-recommend": "11.2.4",
		"@semantic-release/exec": "7.1.0",
		"@semantic-release/git": "10.0.1",
		"@testing-library/dom": "10.4.1",
		"@testing-library/react": "16.3.0",
		"@vitejs/plugin-react": "5.0.2",
		"@vitest/coverage-v8": "3.2.4",
		"jsdom": "26.1.0",
		"secretlint": "11.2.4",
		"turbo": "2.5.6",
		"typedoc": "0.28.12",
		"typedoc-github-wiki-theme": "2.1.0",
		"typedoc-plugin-mark-react-functional-components": "0.2.2",
		"typedoc-plugin-markdown": "4.8.1",
		"vite-tsconfig-paths": "5.1.4",
		"vitest": "3.2.4"
	},
	"packageManager": "bun@1.2.20",
	"scripts": {
		"semantic-release": "bunx semantic-release",
		"docs": "typedoc --options ./.config/typedoc.json"
	},
	"release": {
		"branches": [
			"main"
		],
		"plugins": [
			"@semantic-release/commit-analyzer",
			"@semantic-release/release-notes-generator",
			"@semantic-release/git",
			"@semantic-release/github",
			[
				"@semantic-release/exec",
				{
					"successCmd": "echo '${nextRelease.gitTag}' > version.txt && echo 'released=true' > release_status.txt"
				}
			]
		]
	},
	"trustedDependencies": [],
	"workspaces": [
		"apps/*"
	],
	"dependencies": {
		"dotenv": "17.2.2",
		"lefthook": "1.13.0"
	},
	"resolutions": {
		"esbuild": "^0.25.9"
	}
}
