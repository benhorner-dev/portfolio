{
	"$schema": "https://turborepo.com/schema.json",
	"tasks": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": [".next/**", "!.next/cache/**"],
			"env": ["SENTRY_CLIENT_SECRET", "SLACK_WEBHOOK_URL"]
		},
		"check-types": {
			"dependsOn": ["^check-types"]
		},
		"dev": {
			"persistent": true,
			"cache": false
		},
		"dev:fe": {
			"persistent": true,
			"cache": false
		},
		"test": {},
		"test:unit": {},
		"test:integration": {},
		"test:e2e": {},
		"lint": {},
		"format": {}
	}
}
